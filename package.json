{
  "private": true,
  "scripts": {
    "start-pm2-staging": "pm2 startOrRestart ecosystem.json --only nuxt3-tailwind-template-staging",
    "start-pm2-prod": "pm2 startOrRestart ecosystem.json --only nuxt3-tailwind-template-prod",
    "dev": "nuxt dev",
    "analyze": "npx nuxi analyze",
    "build-dev": "nuxt build",
    "start-dev": "nuxt preview",
    "build-staging": "ANTELOPEJS_LAUNCH_ENV=staging nuxt build",
    "start-staging": "ANTELOPEJS_LAUNCH_ENV=staging PORT=3000 node .output/server/index.mjs",
    "build-prod": "ANTELOPEJS_LAUNCH_ENV=production nuxt build",
    "start-prod": "ANTELOPEJS_LAUNCH_ENV=production PORT=3000 node .output/server/index.mjs",
    "generate": "nuxt generate",
    "lint:ts": "eslint --ext \".ts,.js,.vue\" --ignore-path .gitignore .",
    "lint:ts:fix": "eslint --ext \".ts,.js,.vue\" --ignore-path .gitignore . --fix",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lint:style:fix": "stylelint **/*.{vue,css} --ignore-path .gitignore --fix",
    "lint": "yarn lint:ts && yarn lint:style",
    "lint:fix": "yarn lint:ts && yarn lint:style",
    "postinstall": "patch-package && husky install && nuxt prepare"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.4.0",
    "@headlessui/vue": "^1.7.12",
    "@nuxt/devtools": "^0.2.5",
    "@nuxtjs/color-mode": "^3.2.0",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@nuxtjs/google-fonts": "^3.0.0",
    "@nuxtjs/i18n": "^8.0.0-beta.10",
    "@nuxtjs/tailwindcss": "^6.6.4",
    "@pinia/nuxt": "^0.4.7",
    "@prettier/plugin-pug": "^2.3.0",
    "@tailwindcss/forms": "^0.5.3",
    "@volar/vue-language-plugin-pug": "^1.0.22",
    "@vuelidate/core": "^2.0.0",
    "@vuelidate/validators": "^2.0.0",
    "@vueuse/core": "^9.13.0",
    "@vueuse/nuxt": "^9.13.0",
    "commitlint": "^17.4.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "nuxt": "^3.3.1",
    "nuxt-icon": "^0.3.3",
    "patch-package": "^6.5.1",
    "postcss-custom-properties": "^12.1.11",
    "prettier": "^2.8.1",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "pug": "^3.0.2",
    "sass": "^1.59.3",
    "vite-svg-loader": "^4.0.0"
  }
}
